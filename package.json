{
  "name": "website-framing-proxy",
  "version": "1.0.0",
  "description": "A robust system to enable framing of any website without breaking functionality",
  "main": "dist/server/proxy-server.js",
  "scripts": {
    "build": "tsc --noEmit false",
    "start": "node dist/server/proxy-server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server/proxy-server.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "ws": "^8.14.2",
    "node-cache": "^5.1.2",
    "uuid": "^9.0.1",
    "crypto": "^1.0.1",
    "url": "^0.11.3",
    "querystring": "^0.2.1",
    "@types/cors": "^2.8.17",
    "@types/cookie-parser": "^1.4.6",
    "@types/uuid": "^9.0.7"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/jest": "^29.5.8",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "keywords": [
    "proxy",
    "iframe",
    "framing",
    "reverse-proxy",
    "web-proxy"
  ],
  "author": "Nounspace",
  "license": "MIT"
}
